{
    "name": "chelout/laravel-relationship-events",
    "description": "Missing relationship events for Laravel",
    "homepage": "https://github.com/chelout/laravel-relationship-events",
    "license": "MIT",
    "keywords": [
        "laravel",
        "relationship",
        "relations",
        "events"
    ],
    "authors": [
        {
            "name": "chelout",
            "email": "chelout@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/container": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/events": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.5.4|3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Chelout\\RelationshipEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chelout\\RelationshipEvents\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Chelout\\RelationshipEvents\\RelationshipEventsServiceProvider"
            ]
        }
    }
}